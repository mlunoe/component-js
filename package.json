{
  "name": "webpack-babel-mocha-boilerplate",
  "version": "1.0.0",
  "description": "Webpack, Babel, Mocha + JSDOM boilerplate",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlunoe/webpack-babel-mocha-boilerplate"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --dev-tool eval --progress --colors --hot --content-base build",
