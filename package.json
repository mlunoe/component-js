    "html-webpack-plugin": "2.18.0",
    "jsdom": "9.2.1",
    "mocha": "2.5.3",
    "node-sass": "3.7.0",
    "sass-loader": "3.2.0",
    "sinon": "1.17.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
