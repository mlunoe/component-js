{
  "name": "webpack-babel-mocha-boilerplate",
  "version": "1.0.0",
  "description": "Webpack, Babel, Mocha + JSDOM boilerplate",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mlunoe/webpack-babel-mocha-boilerplate"
  },
  "engines": {
    "node": "4.4.x",
    "npm": "3.9.x"
  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --dev-tool eval --progress --colors --hot --content-base build",
    "deploy": "rm -rf dist && NODE_ENV=production webpack -p",
    "test": "mocha app/**/__tests__/* app/**/__tests__/**/*"
  },
  "author": "https://github.com/mlunoe",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "chai": "3.5.0",
    "css-loader": "0.25.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.5.0",
    "mocha": "3.0.2",
    "node-sass": "3.9.3",
    "postcss-loader": "0.11.1",
    "sass-loader": "4.0.1",
    "sinon": "1.17.5",
    "strict-loader": "1.1.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.1"
  }
